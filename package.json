{
	"name": "hindutempleofstlouis-donation-platform",
	"version": "0.1.0",
	"private": true,
	"dependencies": {
		"body-parser": "^1.18.3",
		"bootstrap": "^4.1.3",
		"express": "^4.16.3",
		"firebase": "^5.5.2",
		"firebase-admin": "^6.0.0",
		"node-sass": "^4.9.3",
		"nodemailer": "^4.6.8",
		"prettier": "^1.14.3",
		"react": "^16.5.2",
		"react-dom": "^16.5.2",
		"react-loading": "^2.0.3",
		"react-scripts": "2.0.1",
		"react-stripe-elements": "^2.0.1",
		"stripe": "^6.12.1"
	},
	"scripts": {
		"start": "node server.js",
		"heroku-postbuild": "npm install && npm install --only=dev --no-shrinkwrap && npm run build",
		"build": "react-scripts build",
		"test": "react-scripts test",
		"format": "prettier --write **/*.{js,json,md,scss,css} !build/** --config .prettierrc",
		"eject": "react-scripts eject",
		"client": "react-scripts start",
		"server": "nodemon server.js",
		"dev": "concurrently --kill-others-on-fail \"yarn server\" \"yarn client\""
	},
	"proxy": "http://localhost:8000/",
	"eslintConfig": {
		"extends": "react-app"
	},
	"browserslist": [
		">0.2%",
		"not dead",
		"not ie <= 11",
		"not op_mini all"
	],
	"devDependencies": {
		"concurrently": "^4.0.1"
